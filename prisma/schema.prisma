// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model songs{
  id Int   @id @default(autoincrement())
  name String?
  artist String
  album String
  year DateTime
  gener String
  duration Int
}